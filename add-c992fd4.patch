++ **/
++#define LOG_MACRO(isAsynchronous, lvl, flg, ctx, atag, fnct, frmt, ...) \
++        [DDLog log : isAsynchronous                                     \
++             level : lvl                                                \
++              flag : flg                                                \
++           context : ctx                                                \
++              file : __FILE__                                           \
++          function : fnct                                               \
++              line : __LINE__                                           \
