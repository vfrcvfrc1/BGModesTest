++#define LOG_MAYBE(async, lvl, flg, ctx, tag, fnct, frmt, ...) \
++        do { if(lvl & flg) LOG_MACRO(async, lvl, flg, ctx, tag, fnct, frmt, ##__VA_ARGS__); } while(0)
