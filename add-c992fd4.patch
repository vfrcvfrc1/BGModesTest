++- (BOOL)isOnGlobalLoggingQueue {
++    return (dispatch_get_specific(GlobalLoggingQueueIdentityKey) != NULL);
++}
++
++- (BOOL)isOnInternalLoggerQueue {
++    void *key = (__bridge void *)self;
++
++    return (dispatch_get_specific(key) != NULL);
++}
